{
    "name": "hello-app",
    "version": "1.0.0",
    "description": "Hello App with Redux",
    "main": "index.js",
    "scripts": {
        "start": "webpack-dev-server --open --hot --mode development",
        "build": "webpack --mode production",
        "lint": "eslint src/**/*.js *.js  && echo 'Lint complete.'",
        "lint:fix": "eslint src/**/*.js  --fix",
        "format": "prettier --write \"**/*.{js,jsx,json}\" \"!.cache/**\" \"!node_modules/**\" \"!public/**\" \"!dist/**\"",
        "format:check": "prettier \"**/*.{js,jsx}\" \"!.cache/**\" \"!node_modules/**\" \"!public/**\" --check",
        "test": "jest --config ./jest.config.js",
        "test:watch": "npm run test -- --watch"
    },
    "keywords": [
        "react",
        "redux",
        "counter"
    ],
    "author": "Babajide Williams",
    "license": "MIT",
    "dependencies": {
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "react-redux": "^7.1.1",
        "redux": "^4.0.4"
    },
    "devDependencies": {
        "@babel/core": "^7.7.2",
        "@babel/plugin-proposal-class-properties": "^7.7.0",
        "@babel/plugin-transform-runtime": "^7.6.2",
        "@babel/preset-env": "^7.7.1",
        "@babel/preset-react": "^7.7.0",
        "@babel/runtime": "^7.7.2",
        "@testing-library/jest-dom": "^4.2.3",
        "babel-eslint": "^10.0.3",
        "babel-jest": "^24.9.0",
        "babel-loader": "^8.0.6",
        "babel-preset-react-app": "^9.0.2",
        "css-loader": "^3.2.0",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "enzyme-to-json": "^3.4.3",
        "eslint": "^6.6.0",
        "eslint-config-prettier": "^6.5.0",
        "eslint-config-react": "^1.1.7",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jest": "^23.0.2",
        "eslint-plugin-prettier": "^3.1.1",
        "eslint-plugin-react": "^7.16.0",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.9.0",
        "jest-styled-components": "^6.3.3",
        "prettier": "^1.18.2",
        "standard": "^14.3.1",
        "style-loader": "^1.0.0",
        "styled-components": "^4.4.1",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.9.0"
